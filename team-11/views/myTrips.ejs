
<table class="table table-striped">
	<thead>
      	<tr>
      		<th> </th>
      		<th>Pick Up Date</th>
        	<th>Driver</th>
        	<th>Fare</th>
        	<th>City</th>
        
      	</tr>
    </thead>
		<tr data-ng-repeat = "ride in rides">
			<td><span class="glyphicon glyphicon-chevron-right"  data-toggle="collapse" data-target="#bill"></span> </td>
 			<td> <p>{{ride.pickUpDate | date:'shortDate'}}</p></td>
			<td> <p>{{ride.driver}}</p></td>
			<td> <p>{{ride.fare | currency}}</p></td>
			<td> <p>{{ride.city}}</p></td>
		</tr>
		<tr><td>
		
  <div id="bill" class="collapse">
